<?xml version="1.0" encoding="UTF-8"?>
<plugin xmlns="http://apache.org/cordova/ns/plugins/1.0"
        id="com.feedhenry.plugins.monkeytalk"
        version="0.0.1">

        <name>MonkeyTalk</name>

                <description>
